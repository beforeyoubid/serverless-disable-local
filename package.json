{
  "name": "basic-template",
  "author": "bailey@beforeyoubid.com.au",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "description": "Service template for GraphQL service",
  "main": "src/handler",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -fr .build",
    "test": "jest --watch --coverage",
    "test:ci": "jest --verbose=false --coverage --passWithNoTests",
    "start:local": "node node_modules/.bin/serverless offline start --stage local",
    "start:debug": "SLS_DEBUG=* node --inspect=0.0.0.0:9229 --nolazy node_modules/.bin/serverless offline start --stage local",
    "lint": "eslint --max-warnings 0 '**/*.{ts,tsx,js,jsx}' && tsc --noEmit",
    "lint:fix": "eslint --fix --max-warnings 0 '**/*.{ts,tsx,js,jsx}' && tsc --noEmit",
    "lint:ci": "eslint --fix --max-warnings 0 '**/*.{ts,tsx,js,jsx}'",
    "types:ci": "tsc --noEmit",
    "lint:graphql-schema": "graphql-schema-linter src/graphql/schema.graphql",
    "prettier:write": "prettier --write '**/*.{ts,tsx,js,jsx,md,json}' && prettier --write '*.{ts,tsx,js,jsx,md,json}'",
    "env-check": "serverless-env-checker --artifact artifacts"
  },
  "graphql-schema-linter": {
    "rules": [
      "defined-types-are-used",
      "deprecations-have-a-reason"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beforeyoubid/service-template-graphq.git"
  },
  "homepage": "https://github.com/beforeyoubid/service-template-graphq#readme",
  "dependencies": {
    "@apollo/subgraph": "2.0.0-alpha.0",
    "@beforeyoubid/error-adapter": "^1.1.3",
    "@beforeyoubid/logger-adapter": "1.0.11",
    "@graphql-tools/utils": "7.6.0",
    "apollo-server-lambda": "3.10.2",
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1214.0",
    "aws-xray-sdk": "^3.3.6",
    "copy-webpack-plugin": "11.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "graphql": "^15",
    "graphql-scalars": "1.18.0",
    "graphql-tag": "2.12.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@apollo/composition": "2.1.1",
    "@apollo/query-graphs": "2.1.1",
    "@auth0/s3": "^1.0.0",
    "@beforeyoubid/graphql-schema-diff": "0.1.1",
    "@beforeyoubid/serverless-env-checker": "^1.0.1",
    "@beforeyoubid/serverless-offline": "^7.1.0",
    "@swc/core": "^1.2.242",
    "@swc/jest": "^0.2.22",
    "@types/async": "3.2.15",
    "@types/aws-lambda": "8.10.103",
    "@types/graphql": "^14.5.0",
    "@types/jest": "29.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "18.7.17",
    "@types/serverless": "3.12.8",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "apollo-server-types": "3.6.2",
    "chance": "^1.1.8",
    "cross-env": "^7.0.3",
    "danger": "11.1.2",
    "danger-plugin-jira-issue": "^1.4.1",
    "danger-plugin-yarn": "1.5.1",
    "deep-equal": "^2.0.5",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql-schema-linter": "3.0.1",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-junit": "14.0.1",
    "nock": "13.2.9",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "rimraf": "^3.0.2",
    "serverless": "3.22.0",
    "serverless-offline": "10.0.2",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-prune-plugin": "2.0.1",
    "serverless-ssm-plugin": "1.0.1",
    "serverless-stack-termination-protection": "2.0.2",
    "serverless-webpack": "5.9.1",
    "serverless-webpack-layers": "2.0.9",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "29.0.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-graphql-loader": "^1.0.0",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "@apollo/federation-internals": "2.0.0-alpha.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:fix"
    }
  }
}
